<html>
	<head>
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no"/>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8"/>
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="images/favicon_2.ico?1" />

    <title>Nome_Progetto</title>

    <!-- Bootstrap core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="css/sticky-footer-navbar.css" rel="stylesheet">
    
  </head>
  <body>
	<header>
      <!-- Fixed navbar -->
      <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
        <h1 class="navbar-brand">Nome_Progetto</h1>
       
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
          <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
              <a class="nav-link" href="/users/home">Home</a>
            </li>
            <li class="nav-item active">
              <a class="nav-link" href="/users/profile">Profilo<span class="sr-only">(current)</span></a>
            </li>
            <li class="nav-item active">
              <a class="nav-link" href="/users/logout">Logout</a>
            </li>
          </ul>
          <div class="clock" id="clock"></div>
        </div>
      </nav>
    </header>
    
    <main role="main" class="container">
    <br>
    <div class="tab-content no-border padding-24">
				<div id="home" class="tab-pane in active">
					<div class="row">
						<div class="col-xs-12 col-sm-3 center">
							<div>
							<span class="profile-picture">
								<img class="editable img-responsive" alt=" Avatar" id="avatar2" src="images/profile.png?1">
							</span>

							<a href="#" class="btn btn-sm btn-block btn-success">
								<i class="ace-icon fa fa-plus-circle bigger-120"></i>
								<span class="bigger-110">Span1</span>
							</a>

							<a href="#" class="btn btn-sm btn-block btn-primary">
								<i class="ace-icon fa fa-envelope-o bigger-110"></i>
								<span class="bigger-110">Span2</span>
							</a>
							</div>
						</div><!-- /.col -->

						<div class="col-xs-12 col-sm-9">
							<h4 class="blue">
								<span class="middle">Profilo</span>

							</h4>

							<div class="profile-user-info">
								<div class="profile-info-row">
									<div class="profile-info-name"> Name <span style="color:darkolivegreen"><%= name %></span> </div>
                  <div class="profile-info-surname"> Surname <span style="color:darkolivegreen"><%= surname %></span> </div>
                  <br><br>
                  <button class="btn btn-outline-primary btn-sm" id = "show_city" type="button">Show city</button>
                  <br><br>
                  <select class="custom-select"  id="list_city" oninput="">
                    <option selected disabled>My City</option>
                  </select>
									
								</div>
								
							</div>

							</div>
						</div><!-- /.col -->
					</div><!-- /.row -->
					</main>
    
    
  <body>
</html>
